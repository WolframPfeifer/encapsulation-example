(declare-abstractions
    ((example.Cell 0))
    (((Cell (absVal Int)))))

(define-contract example.Cell.init
    ((this (out Cell))
     (v (in Int)))
    ((true
     (= (absVal this) v))))

(define-contract example.Cell.value
    ((this (inout Cell))
     (result (out Int)))
    ((true
     (= result (absVal this)))))

(define-contract example.Cell.set
    ((this (inout Cell))
     (v (in Int)))
    ((true
     (= (absVal this) v))))
